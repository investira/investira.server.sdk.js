{
    "name": "investira.server.sdk",
    "version": "1.1.1",
    "author": "Investira",
    "description": "Investira server SDK",
    "main": "index.js",
    "types": "./lib/globals.d.ts",
    "type": "version",
    "registry": true,
    "raw": "investira.server.sdk@1.1.1",
    "escapedName": "investira.server.sdk",
    "rawSpec": "1.1.1",
    "saveSpec": null,
    "fetchSpec": "1.1.1",
    "homepage": "https://investira.com.br/",
    "engines": {
        "node": ">=11.11.0",
        "npm": ">=6.0.0"
    },
    "keywords": [
        "investira.server.sdk",
        "investira"
    ],
    "_requiredBy": [
        "/"
    ],
    "repository": {
        "url": "git://github.com/investira/investira.server.sdk.js.git"
    },
    "bugs": {
        "url": "https://github.com/investira/investira.server.sdk/issues"
    },
    "scripts": {
        "start": "nodemon ./lib/tests",
        "_test": "nodemon --inspect-brk ./_tests/test"
    },
    "dependencies": {
        "@mysql/xdevapi": "8.0.16",
        "@types/node": "11.9.4",
        "bcrypt": "3.0.6",
        "investira.sdk": "^1.1.0",
        "nodemailer": "6.1.1",
        "on-finished": "2.3.0",
        "pug": "2.0.4",
        "winston": "3.2.1",
        "yauzl": "2.10.0"
    },
    "jshintConfig": {
        "curly": true,
        "eqeqeq": false,
        "latedef": false,
        "undef": false,
        "varstmt": true,
        "funcscope": true,
        "supernew": true,
        "boss": true,
        "eqnull": true,
        "strict": false,
        "scripturl": true,
        "maxdepth": 4,
        "maxparams": 5,
        "esversion": 9,
        "globals": {
            "esversion": 9,
            "location": true,
            "printStackTrace": false,
            "gLog": true,
            "gRC": true
        },
        "browser": true,
        "node": true
    },
    "nodemonConfig": {
        "ignore": [
            ".git",
            "node_modules/**/node_modules"
        ],
        "delay": "0"
    },
    "prettier": {
        "singleQuote": true,
        "jsxBracketSameLine": true,
        "bracketSpacing": true,
        "trailingComma": "none",
        "tabWidth": 4,
        "arrowParens": "avoid",
        "printWidth": 120
    },
    "license": "GPL-3.0-only",
    "directories": {
        "lib": "lib"
    }
}
