    "jshintConfig": {
        "curly": true,
        "eqeqeq": false,
        "latedef": false,
        "undef": false,
        "varstmt": true,
        "funcscope": true,
        "supernew": true,
        "boss": true,
        "eqnull": true,
        "strict": false,
        "scripturl": true,
        "maxdepth": 4,
        "maxparams": 5,
        "esversion": 9,
        "globals": {
            "esversion": 9,
            "location": true,
            "printStackTrace": false,
            "gLog": true,
            "gRC": true
        },
        "browser": true,
        "node": true
