    "nodemonConfig": {
        "ignore": [
            ".git",
            "node_modules/**/node_modules"
        ],
        "delay": "0"
    },
    "prettier": {
        "singleQuote": true,
        "jsxBracketSameLine": true,
        "bracketSpacing": true,
        "trailingComma": "none",
        "tabWidth": 4,
        "arrowParens": "avoid",
        "printWidth": 120
    },
    "license": "GPL-3.0-only",
    "directories": {
        "lib": "lib"
